<templateSet group="NanoMessage">
    <template name="click" value="&lt;click:$ACTION$:$DATA$&gt;$END$&lt;/click&gt;" toReformat="false"
              toShortenFQNames="true">
        <variable name="ACTION"
                  expression="enum(&quot;run_command&quot;,&quot;change_page&quot;,&quot;copy_to_clipboard&quot;,&quot;open_file&quot;,&quot;open_url&quot;,&quot;suggest_command&quot;)"
                  defaultValue="&quot;run_command&quot;" alwaysStopAt="true"/>
        <variable name="DATA" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="Translations" value="true"/>
        </context>
    </template>
    <template name="repeat" value="&lt;repeat:$COUNT$&gt;$END$&lt;/repeat&gt;" toReformat="false" toShortenFQNames="true">
        <variable name="DATA" expression="" defaultValue="2" alwaysStopAt="true"/>
        <context>
            <option name="Translations" value="true"/>
        </context>
    </template>
</templateSet>
